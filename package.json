{
  "name": "open-chess",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run run-build && npm run serve",
    "start-claims": "npm run run-build-claims && npm run serve",
    "dev": "cross-env DEBUG=vite-plugin-pwa:* vite",
    "build": "cross-env DEBUG=vite-plugin-pwa:* vite build",
