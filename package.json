    "serve": "serve dist"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chess.js": "^0.12.0",
    "equal-vue": "^0.75.0",
    "lodash": "^4.17.21",
    "parse": "^3.2.0",
    "vue": "^3.0.11",
    "vue-chessboard": "^0.6.7",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vueuse/core": "^5.0.3",
    "cross-env": "^7.0.3",
    "https-localhost": "^4.6.4",
    "typescript": "^4.2.4",
    "vite": "^2.2.3",
    "vite-plugin-pwa": "^0.8.1"
  }
